  Mist_F Const_F 
  Fasz_M Eng_M Sex_M Fut_M
  Mist_M Const_M;

  USEVAR Emot_MT1 Emot_FT1 Fut_F
  Fut_M;


ANALYSIS:
  estimator=ml;
